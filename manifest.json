{
  "manifest_version": 2,

  "name": "Udemy Video Downloader",
  "description": "Download udemy video in current page.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Download Udemy Videos"
  },

  "content_scripts": [
    { "matches": ["https://www.udemy.com/*"], "js": ["udemy_cs.js"] }
  ],

  "permissions": [ "https://www.udemy.com/*" ]
}
